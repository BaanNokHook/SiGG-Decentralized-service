{
  "private": true,
  "version": "1.0.0",
  "license": "EUPL-1.2",
  "scripts": {
    "audit": "audit-ci --moderate",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint-staged": "lint-staged",
    "lint": "npm run lint:js && npm run lint:ts && npm run lint:prettier",
    "lint:js": "eslint \"*/**/*.{js,ts}\"",
    "lint:ts": "tsc --noEmit --incremental false",
    "lint:prettier": "prettier --check \"**/*.{md,json,yml,yaml,js,ts}\"",
    "lint:css": "npx stylelint \"**/*.css\" --fix",
    "test:watch": "jest --watch",
    "test:unit": "jest --clearCache && jest --runInBand --verbose false --testPathPattern=\"^.+\\.(spec|test)\\.ts$\"",
    "test:e2e": "jest --clearCache && jest --config ./tests/jest-e2e.json --runInBand",
    "test": "jest --clearCache && jest --config ./tests/jest-all.json --runInBand --verbose false",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "dependencies": {
    "@cef-SiGG/app-jwt": "0.0.11",
    "@nestjs/common": "^7.4.4",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "^7.4.4",
    "@nestjs/platform-express": "^7.4.4",
    "@nestjs/swagger": "^4.6.0",
    "axios": "^0.19.2",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "ethers": "^5.0.14",
    "express": "^4.17.1",
    "fast-json-patch": "^3.0.0-1",
    "form-data": "^3.0.0",
    "hbs": "^4.1.1",
    "jose": "^1.27.3",
    "nest-winston": "^1.4.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.3",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.1",
    "@nestjs/schematics": "^7.1.2",
    "@nestjs/testing": "^7.4.4",
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.14",
    "@types/node": "^12.12.61",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "audit-ci": "^3.1.1",
    "eslint": "^7.9.0",
    "eslint-config-airbnb-typescript": "^8.0.2",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.18.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "jest-sonar": "^0.2.11",
    "lint-staged": "^10.4.0",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "stylelint": "^13.7.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-prettier": "^1.1.2",
    "supertest": "^4.0.2",
    "ts-jest": "^26.3.0",
    "ts-loader": "^8.0.3",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
